
/*******************************************************************************/
/*******************************************************************************/
class Person is
begin
    var _name: string = "N/A";
    var _age: int = 0;

    /***************************************************************************/
    routine construct(var name: string, var age: int): object
    begin
        this->_name = name;
        this->_age = age;
        return this;
    end

    /***************************************************************************/
    routine GetName(): string
    begin
        return this->_name;
    end

    /***************************************************************************/
    routine SetName(var n: string): void
    begin
        this->_name = n;
    end

    /***************************************************************************/
    routine GetAge(): int
    begin
        return this->_age;
    end

    /***************************************************************************/
    routine SetAge(var age: int): void
    begin
        this->_age = age;
    end

end // class Person

var p: object = Person("Dave", 56);
print(p->GetName());
print(p->GetAge());